<plugin>
  <groupId>com.paypal.selion</groupId>
  <artifactId>SeLion-Code-Generator</artifactId>
  <version>1.0.0</version>
  <executions>
    <execution>
      <phase>generate-sources</phase>
      <goals>
        <goal>generate</goal>
      </goals>
    </execution>
  </executions>
  <configuration>
    <basePackage>com.mycompany</basePackage>
    <htmlCustomElements>
      <htmlCustomElement>
        com.mycompany.CustomButton
      </htmlCustomElement>
    </htmlCustomElements>
    <iosCustomElements>
      <iosCustomElement>
        com.mycompany.ios.CustomUIAButton
      </iosCustomElement>
    </iosCustomElements>
    <androidCustomElements>
      <androidCustomElement>
        com.mycompany.android.CustomUiButton
      </androidCustomElement>
    </androidCustomElements>
    <mobileCustomElements>
      <mobileCustomElement>
        com.mycompany.mobile.CustomButton
      </mobileCustomElement>
    </mobileCustomElements>
  </configuration>
</plugin>