    <test verbose="2" name="WebTest-Threaded-Regression" annotations="JDK">
        <classes>
            <class name="com.paypal.selion.ios.sample.IOSTest"></class>
            <class name="com.paypal.selion.ios.sample.NativeAppDemo"></class>
        </classes>
    </test>
</suite>