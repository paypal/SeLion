<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="IOSDriver Mobile Sample Suite" skipfailedinvocationcounts="false" junit="false"
    parallel="false" data-provider-thread-count="50" annotations="JDK">
    <!-- SELENIUM CONFIGURATION -->
    <parameter name="mobileNodeType" value="ios-driver" />
    <parameter name="seleniumhost" value="localhost" />
    <parameter name="seleniumport" value="5555" />

    <test verbose="2" name="IOSDriver-MobileSampleSuite" annotations="JDK">
        <classes>
            <class name="com.paypal.selion.iosdriver.ios.sample.IOSTest"></class>
            <class name="com.paypal.selion.iosdriver.ios.sample.NativeAppDemo"></class>
        </classes>
    </test>
</suite>
