<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="IOSDriverPageObjectsDemo Suite" skipfailedinvocationcounts="false" junit="false"
    parallel="false" data-provider-thread-count="1" annotations="JDK">
    <!-- SELENIUM CONFIGURATION -->
    <parameter name="mobileNodeType" value="ios-driver" />
    <parameter name="seleniumhost" value="localhost" />
    <parameter name="seleniumport" value="5555" />
    <parameter name="mobileDevice" value="iphone:9.3" />
    <parameter name="mobileDeviceType" value="iPhone6" />

    <test verbose="2" name="appium-ios-page-objects-test" annotations="JDK">
        <classes>
            <class name="com.paypal.selion.iosdriver.ios.sample.IOSDriverLabelAndNavigationTest"/>
            <class name="com.paypal.selion.iosdriver.ios.sample.IOSDriverButtonTapTest"/>
            <class name="com.paypal.selion.iosdriver.ios.sample.IOSDriverButtonTouchTest"/>
            <class name="com.paypal.selion.iosdriver.ios.sample.IOSDriverSliderSwitchTest"/>
            <class name="com.paypal.selion.iosdriver.ios.sample.IOSDriverTableAlertTest"/>
            <class name="com.paypal.selion.iosdriver.ios.sample.IOSDriverPickerTest"/>
        </classes>
    </test>

</suite>

